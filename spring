npm install json-conflict
import React, { useState, useEffect } from 'react';
import jsonConflict from 'json-conflict';

const DropdownList = () => {
  const [keys, setKeys] = useState([]);

  useEffect(() => {
    const fetchData = async () => {
      try {
        const json = await jsonConflict.readFile('path/to/your/json/file.json');
        const jsonKeys = Object.keys(json);
        setKeys(jsonKeys);
      } catch (error) {
        console.error('Error reading JSON file:', error);
      }
    };

    fetchData();
  }, []);

  return (
    <select>
      {keys.map((key, index) => (
        <option key={index} value={key}>
          {key}
        </option>
      ))}
    </select>
  );
};

export default DropdownList;
